{
  "name": "kanade.log",
  "version": "1.0.0",
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@chakra-ui/react": "2.3.5",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@graphql-tools/load-files": "6.6.1",
    "@graphql-tools/merge": "8.3.6",
    "@graphql-tools/schema": "9.0.4",
    "framer-motion": "7.5.1",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "next": "12.2.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.2",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typed-document-node": "2.3.3",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@types/jest": "29.1.1",
    "@types/mocha": "10.0.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "babel-jest": "29.1.2",
    "cspell": "6.12.0",
    "dotenv": "16.0.3",
    "eslint": "8.24.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.8",
    "fixpack": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yaml-loader": "0.8.0"
  },
  "private": true,
  "scripts": {
    "build": "yarn codegen && next build",
    "codegen": "run-s codegen:*",
    "codegen:generate": "graphql-codegen -r ts-node/register -c codegen.yml",
    "codegen:load": "graphql-codegen --require dotenv/config -r ts-node/register -c codegen-json.yaml",
    "confirm": "run-p confirm:*",
    "confirm:format": "prettier '*/**/*.{js,ts,tsx}'",
    "confirm:lint": "yarn eslint '*/**/*.{js,ts,tsx}'",
    "confirm:spell": "cspell '*/**/*.{js,ts,tsx}'",
    "dev": "yarn codegen && next",
    "fix": "run-p fix:*",
    "fix:format": "prettier '*/**/*.{js,ts,tsx}' --write",
    "fix:lint": "yarn eslint  --fix '*/**/*.{js,ts,tsx}'",
    "fix:pack": "fixpack",
    "prepare": "husky install",
    "start": "next start",
    "test": "yarn codegen && jest"
  }
}
